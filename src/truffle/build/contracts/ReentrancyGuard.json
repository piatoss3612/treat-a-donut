  "source": "// SPDX-License-Identifier: UNLICENSED\r\n\r\npragma solidity ^0.8.17;\r\n\r\nabstract contract ReentrancyGuard {\r\n    bool private locked;\r\n\r\n    modifier lock() {\r\n        require(!locked, \"reentrency not allowed\");\r\n        locked = true;\r\n        _;\r\n        locked = false;\r\n    }\r\n}\r\n",
