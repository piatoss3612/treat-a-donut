  "source": "// SPDX-License-Identifier: UNLICENSED\r\n\r\npragma solidity ^0.8.17;\r\n\r\nabstract contract ReentrancyGuard {\r\n    bool private _locked;\r\n\r\n    modifier lock() {\r\n        require(!_locked, \"reentrency detected\");\r\n        _locked = true;\r\n        _;\r\n        _locked = false;\r\n    }\r\n}\r\n",
